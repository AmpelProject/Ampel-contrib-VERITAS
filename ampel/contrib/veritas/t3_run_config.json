{
    "job": "WebViews",
    "schedule": "every().day.at('15:00')",
    "desycloud.default": "https://desycloud.desy.de/remote.php/webdav/",
    "transients": {
        "select": {
            "modified": {
                "after": {
                    "use": "$timeDelta",
                    "arguments": {"days": -1}
                }
            },
            "channels": {"anyOf": [
                "VERITAS_BLAZARS"
                ]
            },
            "withTags": "SURVEY_ZTF",
            "withoutTags": "HAS_ERROR"
        },
        "state": "$latest",
        "content": {
            "docs": ["TRANSIENT", "COMPOUND", "T2RECORD", "PHOTOPOINT", "UPPERLIMIT"]
        },
        "chunk": 200
    },
    "tasks": [
        {
            "task": "WebViews",
            "unitId": "TransientWebPublisher",
            "transients": {
                "select": {
                    "channels": {
                        "anyOf": ["$forEach"]
                    }
                }
            }
        }
    ]
}
